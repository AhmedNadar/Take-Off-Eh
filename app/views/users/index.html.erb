<div class="flex">
  <div class="w-full px-6 markdown xl:px-12 ">
    <h3>Buitl in Toronto Community members</h3>
    <div class="p-4 overflow-hidden bg-blue-100 rounded-t-lg column" >
      <table class="table-auto">
        <thead>
          <tr>
            <th class="px-4 py-2">User Email</th>
            <th class="px-4 py-2">User Full Name</th>
            <th class="px-4 py-2">Slug</th>
            <th class="px-4 py-2">Joind Date</th>
            <th class="px-4 py-2">Delete</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <%= render user %>
          <% end %>  
        </tbody>
      </table>
    </div>
  </div>
</div>


