    <div class="pt-16 pb-16 bg-gray-100">
      <div class="container px-4 mx-auto lg:px-0">
        <h3 class="mb-6 text-3xl font-black text-center font-display">Latest Companies in Toronto</h3>
        <div class="grid justify-center grid-cols-1 gap-6 lg:grid-cols-6">
          <% @companies.each do |company| %>
            <article class="lg:col-span-2">
              <a href="/blog/lifestyle/reducing-meetings-reduces-stress/" class="">
                <%= image_tag company.company_logo.variant(resize_to_limit: [600, 400]), class:"h-auto max-w-full rounded-t", alt:"company.name" if company.company_logo.present? %>
              </a>

              <div class="p-6 overflow-hidden bg-white rounded-b shadow-sm">
                <header>
                  <h1 class="my-2 text-2xl font-black leading-none font-display">
                  <%= link_to company.name, company %>              
                  </h1>

                  <p class="text-sm text-gray-600">Found Date <%= company.found_date %>
                        <span>by <%= link_to company.user.name, company.user, class: "underline", target: "_blank" %> </span>
                  </p>
                </header>
                <div class="mt-2 mb-4 font-serif text-lg">
                  <p><%= truncate(strip_tags(company.details.to_s), length: 140) %></p>
                </div>
                <%= link_to "View Company", company, class: "btn btn-green" %> 
              </div>
            </article>
          <% end %>
        </div>
      </div>
    </div>
  </main>
</div>





