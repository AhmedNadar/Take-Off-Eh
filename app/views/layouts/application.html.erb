<% provide(:title, "Home") %>
<!DOCTYPE html>
<html class="antialiased">
  <%= render "shared/head" %>

  <body class="relative antialiased leading-normal text-gray-800">
    <%= render "shared/flash_notice" %>
    <%= render "shared/header" %>

    <main class="relative overflow-x-hidden">
      <section class="relative px-4 pt-10 lg:pt-20 lg:px-0">
        <%= content_for?(:content) ? yield(:content) : yield %>
      </section>


    </main>
  </body>
</html>

